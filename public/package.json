{
    "name": "webpack-project-config-reference",
    "version": "1.0.0",
    "description": "webpack-project-config-reference",
    "main": "./webpack/webpack.config.dev.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build:dll": "webpack --config ./webpack/dll/webpack.dll.config.js --mode production",
        "build:dev": "SET NODE_ENV=development&& webpack-dev-server --config ./webpack/webpack.config.dev.js --color",
        "build:prod": "SET NODE_ENV=production&& webpack --config ./webpack/webpack.config.prod.js --watch --color",
        "build:unix:dev": "export NODE_ENV=development && webpack-dev-server --config ./webpack/webpack.config.dev.js --color",
        "build:unix:prod": "export NODE_ENV=production && webpack --config ./webpack/webpack.config.prod.js --watch --color",
        "build:dev:https": "SET NODE_ENV=development&& SET WDS_SCHEME=https&& webpack-dev-server --config ./webpack/webpack.config.dev.js --color --https",
        "build:prod:https": "SET NODE_ENV=production&& SET WDS_SCHEME=https&& webpack --config ./webpack/webpack.config.prod.js --watch --color --https",
        "build:unix:dev:https": "export NODE_ENV=development && export WDS_SCHEME=https && webpack-dev-server --config ./webpack/webpack.config.dev.js --color --https",
        "build:unix:prod:https": "export NODE_ENV=production && export WDS_SCHEME=https && webpack --config ./webpack/webpack.config.prod.js --watch --color --https"
    },
    "author": "imwtr",
